{
    "manifest_version": 3,
    "name": "Already Visited Extension",
    "description": "Notifies users if the page they arrived to has been visited before.",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "history",
        "notifications"
    ],
    "action": {
      "default_popup": "hello.html",
      "default_icon": "hello_extension.png"
    }
  }